    "trailingComma": "all",
    "printWidth": 100
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsnl": "^1.0.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "customize-cra": "^0.7.0"
  }
}
