{
  "name": "pea-alias",
  "version": "0.0.1",
  "description": "Pea webpack alias plugin",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsnl dev",
    "build": "tsnl build",
    "test": "tsnl test",
    "test:watch": "tsnl test --watch",
    "test:cov": "tsnl test --coverage",
    "lint": "tsnl lint",
    "lint:fix": "tsnt lint --fix"
