{
  "name": "pea-plugin-alias",
  "version": "0.1.0",
  "description": "Pea webpack alias plugin",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsnl dev",
    "build": "tsnl build",
    "test": "tsnl test",
    "test:watch": "tsnl test --watch",
    "test:cov": "tsnl test --coverage",
    "lint": "tsnl lint",
    "lint:fix": "tsnt lint --fix"
  },
  "eslintConfig": {
    "extends": "tie-app"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsnl": "^1.2.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@peajs/types": "0.0.3",
    "customize-cra": "^0.8.0"
  }
}
