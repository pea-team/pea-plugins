{
  "name": "pea-plugin-less",
  "version": "0.0.1",
  "description": "Pea less plugin",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsnl dev",
    "build": "tsnl build",
    "test": "tsnl test",
    "test:watch": "tsnl test --watch",
    "test:cov": "tsnl test --coverage",
    "lint": "tsnl lint",
    "lint:fix": "tsnt lint --fix"
  },
  "eslintConfig": {
    "extends": "tie-app"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsnl": "^1.2.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "customize-cra": "^0.8.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0"
  }
}
